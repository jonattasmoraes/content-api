// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ApiContent {
  id        Int    @id @default(autoincrement())
  banner    String
  avatar    String
  aboutPage String

  @@map("api_content")
}

model ApiProjects {
  id            Int    @id @default(autoincrement())
  name          String
  year          String
  urlIcon       String
  aboutProject  String
  infos         String
  repositoryUrl String
  deployUrl     String

  @@map("api_projects")
}
